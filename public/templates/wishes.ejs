<div>
<div class="col-md-4">
left
</div>
<div class="col-md-8">
    <div ng-repeat="data in items" class="col-md-4" ng-model="wishlist">
    <div id="product-box">
    	<div id ="individual-products" style="width:100%;height:200px;border:1px solid ;border-radius: 15px; background-color:#ffffff;" >
    	<input id="hiddenattr" type="hidden" value="{{($index+1)%9}}" ;>
		     <div class="col-md-6" style="width:100%;height:30%" >
			     <div class="block">
		    		<label>Name</label>
		    		{{data.FirstName}}
				</div>
				 <div class="block">
		    		<label>Age:</label>
		    		 {{data.CardAge}}
				</div>
				  <div class="block" >
		    		<label>Price: </label>
		    		{{data.Price}}
				</div>
				 <div class="block" >
		    		<label>Description: </label>
		    		{{data.GiftDescription}}
				</div>
		     </div>
		     <div class="col-md-6">
		      <img src={{data.ImageURL}} style="width:150%;height:100%"/>
		     </div>
		    
		    <button type="button" ng-click="addToCart($index)"  ng-init="status='Add to Cart'" ng-class=class>{{status}}</button>
		    
	    </div>
	    <hr>
	    
    </div>
 </div>    


</div>
